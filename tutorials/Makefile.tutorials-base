LIB         += -ltyra
LIBDIRS     += -L$(ENGINEDIR)/bin
INC         += -I$(ENGINEDIR)/inc
INCDEP		+= -I$(ENGINEDIR)/inc -I$(PS2DEV)/ps2sdk/ports/include/freetype2

include ../../Makefile.base

clean-engine:
	cd $(ENGINEDIR) && $(MAKE) cleaner

build-engine:
	cd $(ENGINEDIR) && $(MAKE)

build-release-engine:
	cd $(ENGINEDIR) && $(MAKE) release
